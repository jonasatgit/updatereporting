-- System list by update ID
IF @ReportType = 1
BEGIN
	select Distinct vrs.Name0 as [Name],
				--Ui.Title,
				css.LastErrorCode,
				IE.ErrorDescription,
				IE.Action, IE.InfoURL
	from v_UpdateComplianceStatus css 
	inner join v_R_System vrs on vrs.ResourceID = css.ResourceID
	--inner join v_UpdateInfo ui on ui.CI_ID=css.CI_ID
	left join #tmpInstallErrors IE on IE.ErrorCode = css.LastErrorCode
	where css.ResourceID in (Select ResourceID from v_FullCollectionMembership where CollectionID in (@CollectionIDs))
	and css.status = 2 -- 2 means missing update
	and css.CI_ID = @CI_ID
	and css.LastErrorCode is not null and css.LastErrorCode != 0
	order by vrs.Name0 desc
	---- fix for SQL compat level problem 
	---- https://support.microsoft.com/en-us/help/3196320/sql-query-times-out-or-console-slow-on-certain-configuration-manager-d
	---- Force legacy cardinality for SQL server versions before 2016 SP1 (SQL version less than 13.0.4001.0)
	--OPTION (QUERYTRACEON 9481)
	---- Force legacy cardinality for SQL server versions 2016 SP1 and higher (SQL version equal or greater than 13.0.4001.0)
	--OPTION (USE HINT ('FORCE_LEGACY_CARDINALITY_ESTIMATION'))
END

