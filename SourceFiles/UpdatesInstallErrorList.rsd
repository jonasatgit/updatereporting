END


-- All systems with WSUS scan errors per collection selection and specific error
IF @ReportType = 5
BEGIN
	select Distinct vrs.Name0 as [Name],
				--[Title] = '',
				uss.LastErrorCode,
				IE.ErrorDescription,
				IE.Action, IE.InfoURL
	from v_UpdateScanStatus uss 
	inner join v_R_System vrs on vrs.ResourceID = uss.ResourceID
	--inner join v_UpdateInfo ui on ui.CI_ID=css.CI_ID
	left join #tmpInstallErrors IE on IE.ErrorCode = uss.LastErrorCode
	where uss.ResourceID in (Select ResourceID from v_FullCollectionMembership where CollectionID in (@CollectionIDs))
	and uss.LastErrorCode = @LastErrorCode
	order by vrs.Name0 desc
	---- fix for SQL compat level problem 
	---- https://support.microsoft.com/en-us/help/3196320/sql-query-times-out-or-console-slow-on-certain-configuration-manager-d
