    <DataSet Name="UpdatePrefix">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MonthIndex">
            <Value>=Parameters!MonthIndex.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>---- This query serves two purposes
---- Be able to show rollup prefix in dashboard
---- Populate datestring into month selection parameter
DECLARE @LastRollupPrefix as char(7);
DECLARE @CurrentRollupPrefix as char(7);
DECLARE @SecondTuesdayOfMonth datetime;
--Declare @MonthIndex as int = 0; -- 0 = current month, 1 = previous month

-- Calculate 2nd Tuesday of month to add the correct date string in case install date is missing in v_GS_QUICK_FIX_ENGINEERING
-- Also used to fill the gap between the first day of a month and the next time a rollup will be released
SET @SecondTuesdayOfMonth = (DATEADD(Month, DATEDIFF(Month, 0, GETDATE()), 0) + 6 + 7 - (DATEPART(Weekday, DATEADD(Month, DATEDIFF(Month, 0, GETDATE()), 0)) + (@@DateFirst + 3) + 7) %7)

IF GETDATE() &lt; @SecondTuesdayOfMonth
	BEGIN
		SET @LastRollupPrefix = (SELECT convert(char(7),DATEADD(MONTH,-2-@MonthIndex,GETDATE()),126))
		SET @CurrentRollupPrefix = (SELECT convert(char(7),DATEADD(MONTH,-1-@MonthIndex,GETDATE()),126)) 
	END
ELSE
    BEGIN
		SET @LastRollupPrefix = (SELECT convert(char(7),DATEADD(MONTH,-1-@MonthIndex,GETDATE()),126))
		SET @CurrentRollupPrefix = (SELECT convert(char(7),DATEADD(MONTH,0-@MonthIndex,GETDATE()),126))
	END;

Select @LastRollupPrefix as [LastRollupPrefix]
	  ,@CurrentRollupPrefix as [CurrentRollupPrefix]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="LastRollupPrefix">
          <DataField>LastRollupPrefix</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentRollupPrefix">
          <DataField>CurrentRollupPrefix</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ExcludeProductsByDefault">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
-- query to either add 'Microsoft Defender Antivirus' and 'System Center Endpoint Protection' to the default exclusion list
-- Or the value of "..nothing excluded" in case the products have not been found
-- Add more or different values to the list in case other products should be excluded by default

WITH ProductList AS (
    SELECT DISTINCT CICI.CategoryInstanceName AS [ProductName]
    FROM v_CICategoryInfo CICI
    WHERE CICI.CategoryTypeName = 'Product'
)
SELECT [ProductName]
FROM ProductList
WHERE ProductName IN ('Microsoft Defender Antivirus', 'System Center Endpoint Protection')

UNION ALL

SELECT '..nothing excluded'
WHERE NOT EXISTS (
    SELECT 1
    FROM ProductList
    WHERE ProductName IN ('Microsoft Defender Antivirus', 'System Center Endpoint Protection')
)
ORDER BY [ProductName];</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
