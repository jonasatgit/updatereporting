    <DataSet Name="MissingUpdatesNOTDeployed">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ExcludeProductList">
            <Value>=Parameters!ExcludeProductList.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionIDs">
            <Value>=Parameters!CollectionIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Declare @CollectionIDs as varchar(max) = 'SMS00001'
--Declare @ExcludeProductList as varchar(max) = N'System Center Endpoint Protection;Microsoft Server operating system-21H2;Microsoft Server Operating System-22H2';

Declare @ExcludeProducts as varchar(max) = @ExcludeProductList;
Declare @CollectionIDList as varchar(max) = @CollectionIDs;

With CTE_ProductPieces
	AS
	(
		SELECT 1 AS ID
		,1 AS [StartString]
		,Cast(CHARINDEX(';', @ExcludeProducts,0) as int) AS StopString
		UNION ALL
		SELECT ID + 1
		,StopString + 1
		,Cast(CHARINDEX(';', @ExcludeProducts, StopString + 1) as int)
		FROM CTE_ProductPieces
		WHERE StopString &gt; 0
	)
	,CTE_Products
	AS
	(
		SELECT (SUBSTRING(@ExcludeProducts, StartString,
		CASE WHEN StopString &gt; 0 THEN StopString - StartString
		ELSE LEN(@ExcludeProducts)
		END)) AS Product
		FROM CTE_ProductPieces
	)
	,CTE_ExcludedProductUpdates
    AS
    (
		select ciall.CI_ID
		from v_CICategoryInfo_All as ciall
		inner join CTE_Products products on products.Product = ciall.CategoryInstanceName and ciall.categoryTypeName = 'Product'
    ),
	CTE_CollIDPieces
	AS 
	(
		SELECT 1 AS ID
			,1 AS [StartString]
			,Cast(CHARINDEX(';', @CollectionIDList,0) as int) AS StopString
		UNION ALL
		SELECT ID + 1
						,StopString + 1
						,Cast(CHARINDEX(';', @CollectionIDList, StopString + 1) as int)
		FROM CTE_CollIDPieces
		WHERE StopString &gt; 0
	),
	CTE_CollIDs
	AS
	(
		SELECT (SUBSTRING(@CollectionIDList, StartString, 
									CASE WHEN StopString &gt; 0 THEN StopString - StartString
									ELSE LEN(@CollectionIDList)
					END)) AS CollectionID
		FROM CTE_CollIDPieces 
	),
	CTE_AllSystems as (
		Select Count(ResourceID) as AllSystems 
		from v_FullCollectionMembership fcm 
		inner join CTE_CollIDs on CTE_CollIDs.CollectionID = FCM.CollectionID
	),
	CTE_ResourceIDs as (
		Select ResourceID 
		from v_FullCollectionMembership fcm 
		inner join CTE_CollIDs on CTE_CollIDs.CollectionID = FCM.CollectionID
	)

select 
    COUNT(*) as [SystemsWithUpdatesMissing],
    (select AllSystems from CTE_AllSystems) as [AllSystems]
from (
    select UCS.ResourceID
    from v_UpdateComplianceStatus UCS
    left join v_CITargetedMachines CTM on CTM.CI_ID = UCS.CI_ID and CTM.ResourceID = UCS.ResourceID
    inner join v_CICategoryInfo_All CCI on CCI.CI_ID = UCS.CI_ID and CCI.CategoryTypeName = 'UpdateClassification' 
        and (CCI.CategoryInstanceName = 'Security Updates' or CCI.CategoryInstanceName = 'Critical Updates')
    inner join v_UpdateInfo UPD on UPD.CI_ID = UCS.CI_ID
    where EXISTS (
        Select 1 
        from CTE_ResourceIDs 
        where CTE_ResourceIDs.ResourceID = UCS.ResourceID
    )
    and UCS.Status = 2 
    and CTM.ResourceID is null 
    and UPD.IsSuperseded = 0
    and UCS.CI_ID not in (Select CI_ID from CTE_ExcludedProductUpdates)
    group by UCS.ResourceID
) as systems
---- fix for SQL compat level problem
---- https://support.microsoft.com/en-us/help/3196320/sql-query-times-out-or-console-slow-on-certain-configuration-manager-d
---- Force legacy cardinality for SQL server versions before 2016 SP1 (SQL version less than 13.0.4001.0)
--OPTION (QUERYTRACEON 9481)
---- Force legacy cardinality for SQL server versions 2016 SP1 and higher (SQL version equal or greater than 13.0.4001.0)
--OPTION (USE HINT ('FORCE_LEGACY_CARDINALITY_ESTIMATION'))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SystemsWithUpdatesMissing">
          <DataField>SystemsWithUpdatesMissing</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AllSystems">
          <DataField>AllSystems</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
